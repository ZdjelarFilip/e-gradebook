<Window x:Class="WpfApp1.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Login" Height="270" Width="200">
    <StackPanel Margin="20">
        <Label FontSize="16" HorizontalAlignment="Center" Content="Login"/>
        <Separator/>
        <Label Content="Uporabniško ime"/>
        <TextBox Name="TextBoxUporabniskoIme"></TextBox>
        <Label Content="Geslo"/>
        <PasswordBox Name="PasswordBoxGeslo" Margin="0,0,0,10"/>
        <CheckBox Name="PogojBox">Strinjam se z pogoji</CheckBox>
        <Button Click="prijava_click" Name="ButtonPrijava" Margin="0,10,0,0" BorderThickness="0" Foreground="White" Background="Green" FontSize="14" Content="Prijava"/>
    </StackPanel>
</Window>
